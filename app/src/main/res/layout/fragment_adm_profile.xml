<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ConstraintLayout_adm_profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragmentAdm.AdmProfile">


    <TextView
        android:id="@+id/textView7"
        android:layout_width="168dp"
        android:layout_height="37dp"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="16dp"
        android:text="@string/app_name"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="79dp"
        android:text="@string/title_profile"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="38dp"
        android:text="Nombre"
        app:layout_constraintEnd_toStartOf="@+id/etApellidoDashboard_Adm"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginBottom="8dp"
        android:text="Apellidos"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintStart_toEndOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="263dp"
        android:text="DNI"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:text="Telefono"
        app:layout_constraintBaseline_toBaselineOf="@+id/etTelefonoDashboard_Adm"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginBottom="30dp"
        android:text="Email"
        app:layout_constraintBottom_toTopOf="@+id/btnEditar_dashboard_Adm"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnEditar_dashboard_Adm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="113dp"
        android:layout_marginBottom="113dp"
        android:text="@string/ValueEditarDashboardAdm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/etTelefonoDashboard_Adm" />

    <Button
        android:id="@+id/btnCerrar_dashboard_Adm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="30dp"
        android:text="@string/ValueCerrarDashboarAdm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etEmailDashboard_Adm" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="102dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="42dp"
        android:src="@drawable/anonimo"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <TextView
        android:id="@+id/etNombreDashboard_Adm"
        android:layout_width="91dp"
        android:layout_height="25dp"
        android:layout_marginStart="91dp"
        android:layout_marginEnd="91dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/etDniDashboard_Adm" />

    <TextView
        android:id="@+id/etApellidoDashboard_Adm"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_marginEnd="96dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9" />

    <TextView
        android:id="@+id/etDniDashboard_Adm"
        android:layout_width="91dp"
        android:layout_height="25dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="105dp"
        android:layout_marginBottom="105dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintStart_toStartOf="@+id/etTelefonoDashboard_Adm"
        app:layout_constraintTop_toBottomOf="@+id/etNombreDashboard_Adm" />

    <TextView
        android:id="@+id/etTelefonoDashboard_Adm"
        android:layout_width="91dp"
        android:layout_height="25dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="135dp"
        android:layout_marginBottom="136dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/btnCerrar_dashboard_Adm"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/etApellidoDashboard_Adm" />

    <TextView
        android:id="@+id/etEmailDashboard_Adm"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="192dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="192dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnCerrar_dashboard_Adm"
        app:layout_constraintStart_toStartOf="@+id/etTelefonoDashboard_Adm"
        app:layout_constraintTop_toTopOf="@+id/etApellidoDashboard_Adm" />

</androidx.constraintlayout.widget.ConstraintLayout>

